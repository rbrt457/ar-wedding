{
    "extends": ["stylelint-config-standard", "stylelint-config-prettier", "stylelint-config-clean-order", "stylelint-config-recommended-scss", "stylelint-config-recommended-vue"],
    "rules": {
        "scss/at-if-no-null": null,
        "import-notation": "string",
        "at-rule-no-unknown": [
            true,
            {
                "ignoreAtRules": ["function", "if", "each", "include", "mixin", "error", "return", "else"]
            }
        ],
        "no-invalid-position-at-import-rule": null,
        "selector-id-pattern": null,
        "selector-class-pattern": null
    }
}
